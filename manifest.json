{
    "manifest_version": 3,

    "name": "New Extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "icons":{
        "128":"icon.png"
    },

    "action": { 
        "default_icon": "icon.png",
        "default_popup": "popup.html"
     },

    "permissions": [
    "tabs",
    "bookmarks"
    ],
    "host_permissions": [
    "http://www.blogger.com/",
    "*://*/*"
    ]
}